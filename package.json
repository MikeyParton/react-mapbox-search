{
  "name": "react-mapbox-search",
  "version": "0.3.0",
  "description": "Use Mapbox's places API to search for locations. Comes with a Component that autocompletes with suggestions from Mapbox and a query runner that you can use with your own Component.",
  "main": "dist/index",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "babel --out-dir dist --ignore *.test.js src",
    "test": "jest src",
    "prepublish": "npm run build"
  },
  "repository": "https://github.com/MikeyParton/react-mapbox-search",
  "author": "Mike Parton",
  "license": "MIT",
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.6",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^3.0.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "enzyme": "^2.8.2",
    "jest": "^20.0.4",
    "react-addons-test-utils": "^15.6.0"
  },
  "dependencies": {
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-icons": "^2.2.5",
    "rimraf": "^2.6.1",
    "styled-components": "^2.1.0"
  },
  "files": [
    "dist"
  ]
}
